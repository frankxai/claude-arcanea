{
  "name": "@arcanea/claude",
  "version": "1.0.0",
  "description": "Claude Code integration for Arcanea Intelligence OS - Launch Claude with Guardian agents, skills, and workflows",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "claude-arcanea": "./dist/cli.js",
    "arcanea": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./skills": {
      "types": "./dist/skills/index.d.ts",
      "import": "./dist/skills/index.js"
    },
    "./guardians": {
      "types": "./dist/guardians/index.d.ts",
      "import": "./dist/guardians/index.js"
    }
  },
  "files": [
    "dist",
    "CLAUDE.md",
    "README.md",
    "skills",
    "workflows"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts src/cli.ts --format esm --dts --watch",
    "start": "node dist/cli.js",
    "test": "vitest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "arcanea",
    "claude",
    "claude-code",
    "ai",
    "agents",
    "guardians",
    "mythology",
    "mcp",
    "workflow",
    "automation"
  ],
  "author": "FrankX <frank@frankx.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frankxai/claude-arcanea.git"
  },
  "homepage": "https://arcanea.io/claude",
  "bugs": {
    "url": "https://github.com/frankxai/claude-arcanea/issues"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
